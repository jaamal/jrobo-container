package mainpackage;

import one.implementation.IFoo;

import org.jboss.weld.environment.se.Weld;
import org.jboss.weld.environment.se.WeldContainer;

public class Main {
	private WeldContainer container = new Weld().initialize();
	public static void main(String[] args) {
		(new Main()).run();
	}
	private void run() {
		
		Implementation x = (Implementation) container.instance().select(IFoo.class).get();
		x.action();
	}
}
