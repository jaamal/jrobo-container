package collection;

import java.util.ArrayList;

import javax.enterprise.inject.Default;
import javax.inject.Inject;

public class CollectionsInjection {
	public ArrayList<ContentOfCollection> collection;
	@Inject public CollectionsInjection(ArrayList<ContentOfCollection> collection)
	{
		this.collection = collection;
	}
	public void action()
	{
		for(ContentOfCollection x : collection)
			x.action();
//		collection.action();
	}
}
